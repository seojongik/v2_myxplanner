  "create_statement": "CREATE TABLE `v2_staff_manager` (\n  `manager_contract_id` int(11) NOT NULL AUTO_INCREMENT,\n  `branch_id` varchar(20) DEFAULT NULL,\n  `manager_id` int(11) DEFAULT NULL,\n  `staff_type` varchar(10) DEFAULT NULL,\n  `manager_name` varchar(10) DEFAULT NULL,\n  `manager_phone` varchar(20) DEFAULT NULL,\n  `staff_access_id` varchar(20) DEFAULT NULL,\n  `staff_access_password` varchar(50) DEFAULT NULL,\n  `manager_gender` varchar(10) DEFAULT NULL,\n  `staff_status` varchar(10) DEFAULT NULL,\n  `manager_position` varchar(10) DEFAULT NULL,\n  `min_service_min` int(11) DEFAULT NULL,\n  `svc_time_unit` int(11) DEFAULT NULL,\n  `min_reservation_term` int(11) DEFAULT NULL,\n  `reservation_ahead_days` int(11) DEFAULT NULL,\n  `manager_contract_startdate` date DEFAULT NULL,\n  `manager_contract_enddate` date DEFAULT NULL,\n  `contract_type` varchar(20) DEFAULT NULL,\n  `manager_contract_status` varchar(20) DEFAULT NULL,\n  `severance_pay` varchar(20) DEFAULT NULL,\n  `salary_base` int(11) DEFAULT NULL,\n  `salary_hour` int(11) DEFAULT NULL,\n  `salary_incentive` int(11) DEFAULT NULL,\n  `manager_contract_round` int(11) DEFAULT NULL,\n  `updated_at` timestamp NULL DEFAULT NULL,\n  `manager_birthday` date DEFAULT NULL,\n  `salary_meal` int(11) DEFAULT NULL,\n  `salary_meal_minimum_hours` int(11) DEFAULT NULL,\n  PRIMARY KEY (`manager_contract_id`)\n) ENGINE=InnoDB AUTO_INCREMENT=22 DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_unicode_ci",
