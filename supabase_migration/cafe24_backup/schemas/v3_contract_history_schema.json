  "create_statement": "CREATE TABLE `v3_contract_history` (\n  `branch_id` varchar(20) DEFAULT NULL,\n  `contract_history_id` int(11) NOT NULL AUTO_INCREMENT,\n  `member_id` int(11) DEFAULT NULL,\n  `member_name` varchar(20) DEFAULT NULL,\n  `contract_type` varchar(20) DEFAULT NULL,\n  `contract_id` varchar(10) DEFAULT NULL,\n  `contract_name` varchar(30) DEFAULT NULL,\n  `contract_date` date DEFAULT NULL,\n  `contract_register` timestamp NULL DEFAULT current_timestamp() ON UPDATE current_timestamp(),\n  `payment_type` varchar(10) DEFAULT NULL,\n  `contract_history_status` varchar(100) DEFAULT NULL,\n  `price` int(11) DEFAULT NULL,\n  `contract_credit` int(11) DEFAULT NULL,\n  `contract_LS_min` int(11) DEFAULT NULL,\n  `contract_games` int(11) DEFAULT NULL,\n  `contract_TS_min` int(11) DEFAULT NULL,\n  `contract_term_month` varchar(100) DEFAULT NULL,\n  `contract_credit_expiry_date` date DEFAULT NULL,\n  `contract_LS_min_expiry_date` date DEFAULT NULL,\n  `contract_games_expiry_date` date DEFAULT NULL,\n  `contract_TS_min_expiry_date` date DEFAULT NULL,\n  `contract_term_month_expiry_date` date DEFAULT NULL,\n  `bill_id` int(11) DEFAULT NULL,\n  `pro_id` int(11) DEFAULT NULL,\n  `pro_name` varchar(30) DEFAULT NULL,\n  `old_contract_history_id` int(11) DEFAULT NULL,\n  `portone_payment_id` varchar(100) DEFAULT NULL,\n  PRIMARY KEY (`contract_history_id`),\n  KEY `contract_history_ibfk_1` (`contract_id`) USING BTREE,\n  KEY `member_id` (`member_id`) USING BTREE\n) ENGINE=InnoDB AUTO_INCREMENT=4528 DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_unicode_ci",
