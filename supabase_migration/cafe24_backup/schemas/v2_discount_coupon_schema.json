  "create_statement": "CREATE TABLE `v2_discount_coupon` (\n  `branch_id` varchar(20) DEFAULT NULL,\n  `coupon_id` int(11) NOT NULL AUTO_INCREMENT,\n  `coupon_code` varchar(30) DEFAULT NULL,\n  `member_id` int(11) DEFAULT NULL,\n  `member_name` varchar(30) DEFAULT NULL,\n  `coupon_type` varchar(30) DEFAULT NULL,\n  `discount_ratio` decimal(10,0) DEFAULT NULL,\n  `discount_amt` int(11) DEFAULT NULL,\n  `discount_min` int(11) DEFAULT NULL,\n  `coupon_expiry_date` date DEFAULT NULL,\n  `coupon_issue_date` date DEFAULT NULL,\n  `coupon_description` varchar(30) DEFAULT NULL,\n  `updated_at` timestamp NULL DEFAULT NULL,\n  `coupon_status` varchar(10) DEFAULT NULL,\n  `coupon_use_timestamp` timestamp NULL DEFAULT NULL,\n  `LS_id_issued` varchar(20) DEFAULT NULL,\n  `LS_id_used` varchar(20) DEFAULT NULL,\n  `reservation_id_issued` varchar(30) DEFAULT NULL,\n  `reservation_id_used` varchar(30) DEFAULT NULL,\n  `multiple_coupon_use` varchar(10) DEFAULT NULL,\n  PRIMARY KEY (`coupon_id`)\n) ENGINE=InnoDB AUTO_INCREMENT=83 DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_unicode_ci",
