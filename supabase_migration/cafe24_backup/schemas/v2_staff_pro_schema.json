  "create_statement": "CREATE TABLE `v2_staff_pro` (\n  `pro_contract_id` int(11) NOT NULL AUTO_INCREMENT,\n  `branch_id` varchar(20) DEFAULT NULL,\n  `pro_id` int(11) DEFAULT NULL,\n  `staff_type` varchar(10) DEFAULT NULL,\n  `pro_name` varchar(10) DEFAULT NULL,\n  `pro_phone` varchar(20) DEFAULT NULL,\n  `staff_access_id` varchar(20) DEFAULT NULL,\n  `staff_access_password` varchar(100) DEFAULT NULL,\n  `pro_gender` varchar(10) DEFAULT NULL,\n  `staff_status` varchar(10) DEFAULT NULL,\n  `pro_license` varchar(10) DEFAULT NULL,\n  `min_service_min` int(11) DEFAULT NULL,\n  `svc_time_unit` int(11) DEFAULT NULL,\n  `min_reservation_term` int(11) DEFAULT NULL,\n  `reservation_ahead_days` int(11) DEFAULT NULL,\n  `pro_contract_startdate` date DEFAULT NULL,\n  `pro_contract_enddate` date DEFAULT NULL,\n  `contract_type` varchar(20) DEFAULT NULL,\n  `pro_contract_status` varchar(20) DEFAULT NULL,\n  `severance_pay` varchar(20) DEFAULT NULL,\n  `salary_base` int(11) DEFAULT NULL,\n  `salary_hour` int(11) DEFAULT NULL,\n  `salary_per_lesson` int(11) DEFAULT NULL,\n  `salary_per_lesson_min` int(11) DEFAULT NULL,\n  `salary_per_event` int(11) DEFAULT NULL,\n  `salary_per_event_min` int(11) DEFAULT NULL,\n  `salary_per_promo` int(11) DEFAULT NULL,\n  `salary_per_promo_min` int(11) DEFAULT NULL,\n  `salalry_per_noshow` int(11) DEFAULT NULL,\n  `salary_per_noshow_min` int(11) DEFAULT NULL,\n  `pro_contract_round` int(11) DEFAULT NULL,\n  `updated_at` timestamp NULL DEFAULT NULL,\n  `pro_birthday` date DEFAULT NULL,\n  PRIMARY KEY (`pro_contract_id`)\n) ENGINE=InnoDB AUTO_INCREMENT=32 DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_unicode_ci",
